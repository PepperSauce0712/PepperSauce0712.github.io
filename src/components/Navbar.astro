---
import json from '../content/global.json';


const zh_tw = json["zh_tw"];
const { social } = Astro.props;
let icon;

if (social === "ig"){
    icon = "fi fi-brands-instagram";
}
if (social === "dc"){
    icon = "fi fi-brands-discord";
}
if (social === "github"){
    icon = "fi fi-brands-github";
}
---
<div>
    <button>
        <i style="color:#60656C;" class={icon}></i>
    </button>
</div>